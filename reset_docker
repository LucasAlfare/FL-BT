docker stop $(docker ps -aq) ; docker rm $(docker ps -aq) ; docker rmi -f $(docker images -aq) ; docker volume rm $(docker volume ls -q) ; docker network rm $(docker network ls -q | Where-Object {$_ -ne "bridge" -and $_ -ne "host" -and $_ -ne "none"}) ; docker builder prune -a -f ; docker system prune -a -f --volumes